{
  "name": "tooth-and-pail",
  "version": "1.0.0",
  "description": "t&p",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "mocha -w --compilers js:babel-core/register --require ./test/test-helper.js \"test/**/*.@(js|jsx)\"",
    "runme": "nodemon ./dbserver/dbserver.js ",
    "testme": "node ./test/tap-test.js | tap-spec",
    "test:integration": "nightwatch -g test/integration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kakapo2016-projects/tooth-and-pail"
  },
  "keywords": [],
  "author": "Team Ashley",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "d3": "^3.5.16",
    "if-env": "^1.0.0",
    "knex": "^0.10.0",
    "lodash": "^4.6.1",
    "lomega": "^1.0.0",
    "material-ui": "^0.15.0-alpha.2",
    "moment": "^2.12.0",
    "nightwatch": "^0.8.18",
    "nodemon": "^1.9.1",
    "pg": "^4.5.1",
    "progresso": "^0.3.2",
    "react": "^0.14.7",
    "react-addons": "^0.9.1-deprecated",
    "react-cookie": "^0.4.5",
    "react-dom": "^0.14.7",
    "react-google-charts": "^0.2.1",
    "react-router": "^2.0.1",
    "react-share": "^1.4.1",
    "react-tap-event-plugin": "^0.2.2",
    "react-toggle-display": "^2.0.2",
    "reactcss": "^0.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.1.2",
    "superagent": "^1.8.3",
    "uuid": "^2.0.1"
