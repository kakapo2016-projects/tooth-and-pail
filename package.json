    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.0.0",
    "http-server": "^0.8.5",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "tap-spec": "^4.1.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  }
}
