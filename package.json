{
  "name": "tooth-and-pail",
  "version": "1.0.0",
  "description": "t&p",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "mocha -w --compilers js:babel-core/register --require ./test/test-helper.js \"test/**/*.@(js|jsx)\"",
    "runme": "nodemon ./dbserver/dbserver.js "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kakapo2016-projects/tooth-and-pail"
  },
  "keywords": [],
  "author": "Team Ashley",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "cors": "^2.7.1",
    "if-env": "^1.0.0",
    "knex": "^0.10.0",
    "lodash": "^4.6.1",
    "lomega": "^1.0.0",
    "material-ui": "^0.15.0-alpha.2",
    "nodemon": "^1.9.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.1",
    "react-share": "^1.4.1",
    "react-tap-event-plugin": "^0.2.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.1.2",
    "superagent": "^1.8.3",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.0.0",
    "http-server": "^0.8.5",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  }
